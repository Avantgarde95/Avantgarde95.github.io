{
    "name": "mypage",
    "version": "0.0.1",
    "description": "Hunmin Park's homepage",
    "author": "Hunmin Park",
    "dependencies": {
        "next": "^12.0.9",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-avant": "Avantgarde95/react-avant",
        "@reduxjs/toolkit": "^1.7.1",
        "classnames": "^2.3.1",
        "luminous-lightbox": "^2.4.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-brands-svg-icons": "^5.15.4"
    },
    "devDependencies": {
        "typed-scss-modules": "^5.0.0",
        "sass": "^1.49.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.7.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "npm-run-all": "^4.1.5",
        "node-fetch": "^3.2.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.2",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/node": "^17.0.13"
    },
    "scripts": {
        "task:typing": "typed-scss-modules \"src/**/*.module.{css,scss}\"",
        "task:debug": "next dev",
        "task:start": "next start",
        "task:build": "next build",
        "task:export": "next export -o docs",
        "task:lint": "next lint",
        "task:update": "node --experimental-json-modules Update.mjs",
        "task:hooks": "husky install",
        "debug": "run-p \"task:typing -- --watch\" \"task:debug\"",
        "debug:production": "run-s \"task:typing\" \"task:build\" \"task:start\"",
        "export": "run-s \"task:typing\" \"task:build\" \"task:export\"",
        "lint": "run-s \"task:lint\"",
        "lint:fix": "run-s \"task:lint -- --fix\"",
        "update": "run-s \"task:update\"",
        "prepare": "run-s \"task:hooks\""
    }
}