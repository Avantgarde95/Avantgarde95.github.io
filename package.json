{
    "name": "mypage",
    "version": "0.0.1",
    "description": "Hunmin Park's homepage",
    "author": "Hunmin Park",
    "dependencies": {
        "next": "^12.0.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-avant": "Avantgarde95/react-avant",
        "@reduxjs/toolkit": "^1.7.0",
        "classnames": "^2.3.1",
        "luminous-lightbox": "^2.4.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-brands-svg-icons": "^5.15.4"
    },
    "devDependencies": {
        "typed-scss-modules": "^4.1.1",
        "sass": "^1.44.0",
        "eslint-config-airbnb": "^19.0.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsdoc": "^37.2.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "npm-run-all": "^4.1.5",
        "node-fetch": "^3.1.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.7",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/node": "^16.11.12"
    },
    "scripts": {
        "task:typing": "tsm \"src/**/*.module.{css,scss}\"",
        "task:debug": "next dev -p 8080",
        "task:start": "next start -p 8080",
        "task:build": "next build",
        "task:export": "next export -o docs",
        "task:lint": "next lint",
        "task:update": "node --experimental-json-modules Update.mjs",
        "task:hooks": "husky install",
        "serve": "run-p \"task:typing -- --watch\" \"task:debug\"",
        "serve:production": "run-s \"task:typing\" \"task:build\" \"task:start\"",
        "export": "run-s \"task:typing\" \"task:build\" \"task:export\"",
        "lint": "run-s \"task:lint\"",
        "lint:fix": "run-s \"task:lint -- --fix\"",
        "update": "run-s \"task:update\"",
        "prepare": "run-s \"task:hooks\""
    }
}